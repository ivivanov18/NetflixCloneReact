{
  "name": "netflixclonereact",
  "version": "1.0.0",
  "description": "A netflix clone in react and typescript",
  "main": "built/index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "test": "jest",
    "format": "prettier -w"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "setupFiles": [
      "raf/polyfill"
    ],
    "testRegex": ".test.(ts|tsx|js)$",
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ivivanov18/NetflixCloneReact.git"
  },
  "keywords": [
    "netflix",
    "clone",
    "react",
    "typescript",
    "firebase"
  ],
  "author": "Ivan Ivanov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ivivanov18/NetflixCloneReact/issues"
  },
  "homepage": "https://github.com/ivivanov18/NetflixCloneReact#readme",
  "devDependencies": {
    "@types/enzyme": "^3.10.7",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.14",
    "awesome-typescript-loader": "^5.2.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "raf": "^3.4.1",
    "ts-jest": "^26.4.0",
    "typescript": "^4.0.3",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "axios": "^0.20.0",
    "prettier": "^2.1.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  }
}
